{
  "name": "sq-bootstrap-sass",
  "homepage": "https://stash.corp.squareup.com/projects/SQ/repos/sq-bootstrap-sass",
  "authors": [
    "Thomas McDonald",
    "Tristan Harward",
    "Peter Gumeson",
    "Gleb Mazovetskiy",
    "Rick Song"
